provider_key_file = "../key.json"
cloud_id         = "b1g3oxxxxxxxxxxxxxxx"
folder_id        = "b1g7jxxxxxxxxxxxxxxx"
zone             = "ru-central1-a"

vm_ids           = ["vm-id-1", "vm-id-2", "vm-id-3"] # список идентификаторов ВМ для запуска по расписанию
cron_trigger     = "0-5 8 ? * * *"  # Используется async операция для запуска, увеличьте кол-во минут, если список машин большой – например 0-10 для запуска машин по списку каждую минут